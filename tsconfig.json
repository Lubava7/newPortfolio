{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "types": ["vite/client"]
  },
  "include": ["script/*.ts", "script/*.d.ts", "script/*.tsx"],
  "exclude": ["node_modules"]
}
